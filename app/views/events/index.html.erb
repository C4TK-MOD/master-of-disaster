<div>
    <div style="text-align: center">
        <h1>Events</h1>
    </div>

    <div>
        <div>
            <h3><span class="pull-left">Austin Flooding</span><span class="pull-right"><small>10 people needed</small></span>
        </div>
        <br class="clearfix" />
        <div>
            <img src="/assets/austinflood.jpg" style="position: absolute; left: 0px; clip: rect(20px, 360px, 170px, 0px);">
        &nbsp;
        </div>
    </div>
<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <!--<th>Start time</th>
    <th>End time</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>-->
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= event.name %></td>
    <td><%= event.description %></td>
    <!--<td><%= event.start_time %></td>
    <td><%= event.end_time %></td>
    <td><%= event.city %></td>
    <td><%= event.state %></td>
    <td><%= event.zip %></td>-->
    <td><%= link_to 'Show', event %></td>
    <td><%= link_to 'Edit', edit_event_path(event) %></td>
    <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Report Event', new_event_path %>
</div>