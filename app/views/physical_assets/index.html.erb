<h1>Listing physical_assets</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @physical_assets.each do |physical_asset| %>
  <tr>
    <td><%= physical_asset.name %></td>
    <td><%= physical_asset.category.name %></td>
    <td><%= link_to 'Show', physical_asset %></td>
    <td><%= link_to 'Edit', edit_physical_asset_path(physical_asset) %></td>
    <td><%= link_to 'Destroy', physical_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

